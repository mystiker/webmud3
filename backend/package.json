{
  "name": "@webmud3/backend",
  "version": "1.0.0-alpha",
  "description": "Webmud3 Backend Service",
  "engines": {
    "node": "20.12.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/unitopia-de/webmud3.git"
  },
  "main": "main.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "build:watch": "npm run clean && tsc --watch",
    "clean": "rimraf ./dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "postbuild": "cpx package.json dist",
    "start": "npm run build && node --env-file .env dist/main.js",
    "serve": "node dist/main.js",
    "serve:env": "node --env-file .env dist/main.js",
    "test": "jest --config=jest.config.cjs"
  },
  "dependencies": {
    "ace-builds": "~1.33.1",
    "body-parser": "~1.20.2",
    "cookie-session": "~2.1.0",
    "cors": "~2.8.5",
    "express": "~4.19.2",
    "socket.io": "~4.7.5",
    "socket.io-client": "~4.7.5",
    "telnet-stream": "~1.1.0",
    "url": "~0.11.3",
    "uuid": "~9.0.1"
  },
  "devDependencies": {
    "@types/cookie-session": "~2.0.49",
    "@types/express": "~4.17.21",
    "@types/jest": "~29.5.12",
    "@types/node": "20.12.2",
    "@types/source-map-support": "~0.5.10",
    "@types/uuid": "~9.0.8",
    "@typescript-eslint/eslint-plugin": "~7.8.0",
    "@typescript-eslint/parser": "~7.8.0",
    "cpx": "~1.5.0",
    "eslint": "~8.57.0",
    "eslint-config-standard": "~17.1.0",
    "eslint-plugin-import": "~2.29.1",
    "eslint-plugin-node": "~11.1.0",
    "jest": "~29.7.0",
    "prettier": "~3.2.5",
    "rimraf": "~5.0.7",
    "source-map-support": "~0.5.21",
    "ts-jest": "~29.1.2",
    "typescript": "~5.4.5",
    "winston": "~3.13.0",
    "ts-jest-mock-import-meta": "^1.2.0"
  },
  "type": "module",
  "author": "Myonara",
  "license": "MIT"
}
